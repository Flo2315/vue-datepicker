      "require": "./dist/vue-calendar-3.umd.cjs"
    },
    "./style": "./dist/index.css"
  },
  "unpkg": "./dist/vue-calendar-3.umd.cjs",
  "jsdelivr": "./dist/vue-calendar-3.umd.cjs",
  "files": [
    "dist/*",
    "types/*"
  ],
  "types": "./types/main.d.ts",
  "peerDependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
